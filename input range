<body>
   <input id="elem" data-min="5" data-max="10">
    <script>
document.querySelector('#elem').onblur = function () {

 if (String(this.value.length = elem.getAttribute('data-min')) < 5 || 
String(this.value.length = elem.getAttribute('data-max')>= 10)) {
              alert('wrong!')
       }
}
</script>   
    
    
